<Application
    x:Class="TwitterApiProject.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TwitterApiProject"
     xmlns:Converters="clr-namespace:TwitterApiProject.Converters"
    xmlns:vm="clr-namespace:TwitterApiProject.Helpers"
    RequestedTheme="Light">
    
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator" d:IsDataSource="True" />
            <Converters:FrameConverter x:Key="FrameConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
